{
    "configurations": [
        {
            "name": "Main Build",
            "type": "jps-build",
            "workingDir": "$PROJECT_DIR$",
            "module": "com.varunsingh.frequencyanalysissimulator",
            "includeTests": false,
        },  {
        "name": "Main",
        "type": "jps-run",
        "workingDir": "$PROJECT_DIR$",
        "dependsOn": ["Build"],
        "mainClass": "frequencyanalysissimulator.presentation.main.Main",
        "module": "com.varunsingh.frequencyanalysissimulator",
        "options": ["-Dfile.encoding=UTF-8","-Dsun.stdout.encoding=UTF-8","-Dsun.stderr.encoding=UTF-8"],
    },{
        "name": "Data Collection Tool Build",
        "type": "jps-build",
        "workingDir": "$PROJECT_DIR$",
        "module": "com.varunsingh.frequencyanalysissimulator",
        "includeTests": false,
    },

        {
        "name": "Data Collection Tool",
        "type": "jps-run",
        "workingDir": "$PROJECT_DIR$",
        "dependsOn": ["Build"],
        "mainClass": "dataanalysis.DataPopulater",
        "module": "com.varunsingh.frequencyanalysissimulator",
        "options": ["-Dfile.encoding=UTF-8","-Dsun.stdout.encoding=UTF-8","-Dsun.stderr.encoding=UTF-8"],
        "args": ["all"]
    }
    ]
}